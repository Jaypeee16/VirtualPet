<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.virtuaalpet.virtualpet.MainMenu">
   <children>
      <ImageView fitHeight="500.0" fitWidth="800.0">
         <image>
            <Image url="@Pic/Start-up (2).png" />
         </image>
      </ImageView>
      <Button fx:id="playButton" onAction="#handlePlayButton" alignment="CENTER" layoutX="189.0" layoutY="272.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="134.0" style="-fx-background-color: transparent;" text="PLAY" textAlignment="CENTER">
         <font>
            <Font name="Super Legend Boy" size="15.0" />
         </font>
      </Button>
      <Button fx:id="quitButton" onAction="#handleQuitButton" alignment="CENTER" layoutX="487.0" layoutY="272.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="134.0" style="-fx-background-color: transparent;" text="QUIT" textAlignment="CENTER">
         <font>
            <Font name="Super Legend Boy" size="15.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="338.0" layoutY="272.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="134.0" style="-fx-background-color: transparent;" text="SETTINGS" textAlignment="CENTER">
         <font>
            <Font name="Super Legend Boy" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
